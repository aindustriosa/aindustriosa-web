<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <meta name="description" content="Somos un centro de investigación e educación con sede en Vigo onde makers, comunidades tecnolóxicas e empresas, dispoñen do equipamento necesario para poder levar a cabo os seus proxectos.">
    <meta name="author" content="">
    <link rel="icon" href="/i/favicon/favicon.ico">

    <%
    var title = page.title;

    if (is_archive()) {
        title = 'Blog'

        if (is_month()) {
            title += ': ' + page.year + '/' + page.month;
        } else if (is_year()) {
            title += ': ' + page.year;
        }
    } else if (is_category()) {
        title = __('Categoría') + ': ' + page.category;
    } else if (is_tag()) {
        title = __('tag') + ': ' + page.tag;
    }
    %>
    <title>
        <% if (title){ %><%= title %> |
        <% } %>
        <%= config.title %></title>

    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
    <%- open_graph({
        image: page.cover || '',
        title: page.title,
        description: page.description,
        twitter_card: 'summary_large_image',
        twitter_image: (config.url + url_for(page.cover || ''))
    }) %>

    <%- css('css/theme.css') %>
    
    <!-- Google tag (gtag.js) -->
<script async src="https://www.googletagmanager.com/gtag/js?id=G-HHREKELB7J"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'G-HHREKELB7J');
</script>

</head>
